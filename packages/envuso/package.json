{
  "name": "@envuso/cli",
  "description": "Envuso CLI, make a project, generate framework files etc",
  "version": "0.0.4",
  "author": "Sam Parton @iDevelopThings",
  "scripts": {
    "run": "bin/run",
    "postpack": "rm -f oclif.manifest.json",
    "prepack": "rm -rf lib && tsc -b && oclif-dev manifest && oclif-dev readme",
    "test": "nyc --extension .ts mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif-dev readme && git add README.md"
  },
  "bin": {
    "envuso": "bin/run"
  },
  "bugs": "https://github.com/envuso/cli/issues",
  "dependencies": {
    "@oclif/command": "^1",
    "@oclif/config": "^1",
    "@oclif/plugin-autocomplete": "^0.3.0",
    "@oclif/plugin-commands": "^1.3.0",
    "@oclif/plugin-help": "^3.2.2",
    "@oclif/plugin-not-found": "^1.2.4",
    "@oclif/plugin-warn-if-update-available": "^1.7.0",
    "@types/execa": "^2.0.0",
    "@types/inquirer": "^7.3.1",
    "@types/listr": "^0.14.2",
    "@types/lodash": "^4.14.168",
    "boxen": "^5.0.1",
    "execa": "^5.0.0",
    "git-clone": "^0.1.0",
    "inquirer": "^8.0.0",
    "listr": "^0.14.3",
    "lodash": "^4.17.21",
    "rxjs": "^6.6.7",
    "terminal-link": "^2.1.1",
    "tslib": "^1",
    "chalk": "^2.4.2"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1",
    "@oclif/test": "^1",
    "@types/chai": "^4",
    "@types/mocha": "^5",
    "@types/node": "^10",
    "chai": "^4",
    "globby": "^10",
    "mocha": "^5",
    "nyc": "^14",
    "ts-node": "^8",
    "typescript": "^3.3"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/bin",
    "/lib",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/envuso/cli",
  "keywords": [
    "oclif"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "envuso",
    "topics": {
      "make": {
        "description": "Create controllers, models, middleware etc for your application."
      }
    },
    "plugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-warn-if-update-available",
      "@oclif/plugin-not-found",
      "@oclif/plugin-commands",
      "@oclif/plugin-autocomplete"
    ],
    "warn-if-update-available": {
      "timeoutInDays": 1,
      "message": "<%= config.name %> update available from <%= chalk.greenBright(config.version) %> to <%= chalk.greenBright(latest) %>.",
      "registry": "https://www.npmjs.com/package/@envuso/cli"
    }
  },
  "repository": "envuso/cli",
  "types": "lib/index.d.ts"
}
