{
  "version": "0.1.5-alpha.12",
  "name": "@envuso/core",
  "author": "Sam Parton @iDevelopThings",
  "description": "Envuso is a backend framework focusing building apis using Fastify and MongoDB support.",
  "license": "MIT",
  "scripts": {
    "build": "tsc -b --clean && tsc -b",
    "build-commit-version": "yarn build && git add . && git commit -m \":package: Build before versioning\" && npm version prerelease --preid=alpha",
    "commit-files-for-publish": "cp package.json dist && cp readme.md dist && git add . && git commit -m \":package: Build and Increased version\"",
    "prep": "yarn build-commit-version && yarn commit-files-for-publish",
    "link": "yarn build && cp package.json dist && cp readme.md dist && cd dist && yarn link",
    "dist": "yarn build && cp package.json dist && cp readme.md dist && cd dist && yarn publish",
    "preparereeee": "npm run build && npm version prerelease --preid=alpha",
    "test": "jest --roots src",
    "serve": "nodemon dist/main.js",
    "dev": "ts-node ./src/Core/run-dev.ts",
    "push": "yarn build && cp package.json dist/package.json && cd dist && yalc push"
  },
  "dependencies": {
    "@babel/core": "^7.13.14",
    "@babel/plugin-proposal-class-properties": "^7.13.0",
    "@babel/plugin-proposal-decorators": "^7.13.5",
    "@babel/plugin-proposal-private-property-in-object": "^7.13.0",
    "@babel/plugin-transform-classes": "^7.13.0",
    "@babel/preset-env": "^7.13.15",
    "@babel/preset-typescript": "^7.13.0",
    "@zishone/dotnotate": "^0.1.3",
    "aws-sdk": "^2.884.0",
    "babel-loader": "^8.2.2",
    "babel-plugin-replace-ts-export-assignment": "^0.0.2",
    "bcrypt": "^5.0.1",
    "chalk": "^4.1.0",
    "class-transformer": "^0.4.0",
    "class-validator": "^0.13.1",
    "cookies": "^0.8.0",
    "dayjs": "^1.10.4",
    "dotenv": "^8.2.0",
    "fastify": "^3.14.1",
    "fastify-cors": "^5.2.0",
    "fastify-helmet": "^5.3.1",
    "fastify-multipart": "^4.0.3",
    "file-type": "^16.3.0",
    "glob": "^7.1.6",
    "http-status-codes": "^2.1.4",
    "jsonwebtoken": "^8.5.1",
    "lodash.get": "^4.4.2",
    "lodash.has": "^4.5.2",
    "lodash.set": "^4.3.2",
    "middie": "^5.2.0",
    "mongodb": "^3.6.5",
    "node-cache-redis": "^6.3.1",
    "node-fetch": "^2.6.1",
    "pluralize": "^8.0.0",
    "read-chunk": "^3.2.0",
    "redis": "^3.1.2",
    "reflect-metadata": "^0.1.10",
    "simple-crypto-js": "^3.0.0",
    "tsyringe": "^4.5.0",
    "winston": "^3.3.3",
    "winston-daily-rotate-file": "^4.5.2"
  },
  "devDependencies": {
    "@types/busboy": "^0.2.3",
    "@types/connect": "^3.4.34",
    "@types/dotenv": "^8.2.0",
    "@types/glob": "^7.1.3",
    "@types/jest": "^26.0.22",
    "@types/jsonwebtoken": "^8.5.1",
    "@types/lodash.get": "^4.4.6",
    "@types/lodash.has": "^4.5.6",
    "@types/lodash.set": "^4.3.6",
    "@types/mongodb": "^3.6.12",
    "@types/node": "^14.14.41",
    "@types/pluralize": "^0.0.29",
    "@types/redis": "^2.8.28",
    "babel-jest": "^26.6.3",
    "fastify-multipart": "^4.0.3",
    "jest": "^26.6.3",
    "ts-jest": "^26.5.4",
    "typescript": "^4.2.4"
  },
  "files": [
    "AppContainer/**/*",
    "Authentication/**/*",
    "Common/**/*",
    "Core/**/*",
    "Database/**/*",
    "Routing/**/*",
    "Storage/**/*",
    "Cache/**/*",
    "Crypt/**/*",
    "index.*"
  ],
  "types": "./"
}
