    "qs": "^6.10.1",
    "random-bytes": "^1.0.0",
    "read-chunk": "^3.2.0",
    "reflect-metadata": "^0.1.10",
    "ts-node": "^10.2.1",
    "ts-node-dev": "^1.1.8",
    "tslog": "^3.2.1",
    "tsyringe": "^4.5.0",
    "uWebSockets.js": "uNetworking/uWebSockets.js#v19.3.0",
    "utf-8-validate": "^5.0.4",
    "winston": "^3.3.3",
    "winston-daily-rotate-file": "^4.5.2",
    "ws": "^7.4.5",
    "is-image": "^3.0.0",
    "is-video": "^1.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.13.14",
