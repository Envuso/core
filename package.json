    "Authentication/**/*",
    "Authorization/**/*",
    "Cache/**/*",
    "Cli/**/*",
    "Common/**/*",
    "Contracts/**/*",
    "Cookies/**/*",
    "Crypt/**/*",
    "Database/**/*",
